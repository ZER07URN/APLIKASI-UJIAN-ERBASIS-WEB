{
  "name": "aplikasiujianberbasisweb",
  "version": "2.0.0",
  "description": "A Computer Assisted Test Apps with REST API concept for exchange data between backend (NodeJS) and frontend (VueJS)",
  "author": "egodasa <egodasa@gmail.com>",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o public/dist/build.js",
    "serve": "node ./bin/www",
    "dev": "npm-run-all --parallel watchify serve",
    "start": "node ./bin/www",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.4",
    "express-fileupload": "^0.1.4",
    "express-validator": "^3.2.1",
    "jade": "^1.11.0",
    "knex": "^0.13.0",
    "morgan": "^1.8.2",
    "mysql": "^2.14.1",
    "pg": "^7.3.0",
    "promise": "^8.0.1",
    "serve-favicon": "^2.4.3",
    "url": "^0.11.0",
    "vue": "^2.0.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^2.0.0",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "npm-run-all": "^2.3.0",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  }
}
